<?xml version="1.0" encoding="UTF-8"?>
<muclient>
  <aliases>
    <alias
       match="#gmcpdebug"
       enabled="y"
       send_to="12"
       sequence="100"
      >
      <send>
        gmcpdebug = not gmcpdebug
      </send>
    </alias>
    <alias
       match="#reinstall"
       enabled="y"
       send_to="12"
       sequence="100"
      >
      <send>
        Note("attempting to reinstall the main script")
        ReloadPlugin (GetPluginID())
        Note("done")
      </send>
    </alias>
  </aliases>
</muclient>